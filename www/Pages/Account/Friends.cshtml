@page "{id}"
@model www.Pages.Account.FriendsModel
@{
    ViewData["Title"] = "Друзья";
}

<h1>@ViewData["Title"]</h1>
<div>
    @foreach (var user in Model.Users)
    {
        <a class="nav-link text-dark" asp-page="/Account/Profile" asp-route-id="@user.Id">@user.Name @user.Surname</a>
    }
</div>